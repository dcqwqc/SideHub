<Window x:Class="SideHub.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SideHub"
        mc:Ignorable="d"
        WindowStyle="None"
        Background="Transparent"
        AllowsTransparency="True"
        Title="MainWindow" Height="1000" Width="500">

    <Border Background="#2b2943" CornerRadius="15">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="108"/>
                <RowDefinition Height="143*"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="687*"/>
                <RowDefinition Height="30"/>
            </Grid.RowDefinitions>






















            <!-- Audio Integration -->
            <Grid>
                <Rectangle Grid.Row="0" Grid.Column="0" Fill="#ffffff" Margin="20,20,235,0" RadiusX="7" RadiusY="7" x:Name="maskRectangle"/>

                <Image x:Name="mediaThumbnailImage" Margin="20,20,392,0">
                    <Image.OpacityMask>
                        <VisualBrush Visual="{Binding ElementName=maskRectangle}" />
                    </Image.OpacityMask>
                </Image>
            </Grid>



            <Button Name="PlayButton" Background="Transparent" Click="PlayClick" Margin="183,78,289,10" BorderBrush="Transparent" BorderThickness="0">
                <Button.FocusVisualStyle>
                    <Style TargetType="Control">
                        <Setter Property="Background" Value="Transparent"/>
                    </Style>
                </Button.FocusVisualStyle>
                <Viewbox Stretch="Uniform">
                    <Canvas Name="play" Width="20" Height="20">
                        <Canvas.RenderTransform>
                            <TranslateTransform X="0" Y="0"/>
                        </Canvas.RenderTransform>
                        <Canvas.Resources/>
                        <!-- Unknown tag: sodipodi:namedview -->
                        <Path x:Name="playicon" Fill="Black" Stretch="Uniform" Height="14" Width="15" Canvas.Left="4.574" HorizontalAlignment="Left" Canvas.Top="3.667" VerticalAlignment="Top">
                            <Path.Data>
                                <PathGeometry Figures="M9.25 5.06699C9.58333 5.25944 9.58333 5.74056 9.25 5.93301L0.999999 10.6962C0.666666 10.8886 0.25 10.648 0.25 10.2631L0.250001 0.73686C0.250001 0.35196 0.666668 0.111397 1 0.303847L9.25 5.06699Z" FillRule="NonZero"/>
                            </Path.Data>
                        </Path>
                    </Canvas>
                </Viewbox>
            </Button>

            <Button x:Name="skipButton" Background="Transparent" Click="SkipClick" Margin="218,78,254,10" BorderBrush="Transparent" BorderThickness="0">
                <Button.FocusVisualStyle>
                    <Style TargetType="Control">
                        <Setter Property="Background" Value="Transparent"/>
                    </Style>
                </Button.FocusVisualStyle>
                <Viewbox Stretch="Uniform">
                    <Canvas Name="skip" Width="19" Height="13">
                        <Canvas.RenderTransform>
                            <TranslateTransform X="0" Y="0"/>
                        </Canvas.RenderTransform>
                        <Canvas.Resources/>
                        <!-- Unknown tag: sodipodi:namedview -->
                        <Path x:Name="skipicon" Fill="#000000" Stretch="Uniform" Canvas.Left="2.099" Canvas.Top="3.403" HorizontalAlignment="Left" Height="9" VerticalAlignment="Top" Width="6">
                            <Path.Data>
                                <PathGeometry Figures="M9.25 5.06699C9.58333 5.25944 9.58333 5.74056 9.25 5.93301L0.999999 10.6962C0.666666 10.8886 0.25 10.648 0.25 10.2631L0.250001 0.73686C0.250001 0.35196 0.666668 0.111397 1 0.303847L9.25 5.06699Z" FillRule="NonZero"/>
                            </Path.Data>
                        </Path>
                        <Path x:Name="skipicon_Copy" Fill="#000000" Stretch="Uniform" HorizontalAlignment="Left" Canvas.Left="5.932" VerticalAlignment="Top" Canvas.Top="3.403" Height="9" Width="6">
                            <Path.Data>
                                <PathGeometry Figures="M9.25 5.06699C9.58333 5.25944 9.58333 5.74056 9.25 5.93301L0.999999 10.6962C0.666666 10.8886 0.25 10.648 0.25 10.2631L0.250001 0.73686C0.250001 0.35196 0.666668 0.111397 1 0.303847L9.25 5.06699Z" FillRule="NonZero"/>
                            </Path.Data>
                        </Path>
                    </Canvas>
                </Viewbox>
            </Button>


            <Button x:Name="prevButton" Background="Transparent" FontSize="14" Click="PrevClick" Margin="152,78,320,10" BorderBrush="Transparent" BorderThickness="0">
                <Button.FocusVisualStyle>
                    <Style TargetType="Control">
                        <Setter Property="Background" Value="Transparent"/>
                    </Style>
                </Button.FocusVisualStyle>
                <Viewbox Stretch="Uniform">
                    <Canvas Name="prev_mirrored" Width="20" Height="12">
                        <Canvas.RenderTransform>
                            <TranslateTransform X="0" Y="0"/>
                        </Canvas.RenderTransform>
                        <Canvas.Resources/>
                        <!-- Mirrored Path for previcon -->
                        <Path x:Name="previcon_mirrored" Fill="#000000" Stretch="Uniform" Canvas.Left="2.099" Canvas.Top="3.403" HorizontalAlignment="Left" Height="9" VerticalAlignment="Top" Width="6">
                            <Path.RenderTransform>
                                <ScaleTransform ScaleX="-1" CenterX="3"/>
                            </Path.RenderTransform>
                            <Path.Data>
                                <PathGeometry Figures="M9.25 5.06699C9.58333 5.25944 9.58333 5.74056 9.25 5.93301L0.999999 10.6962C0.666666 10.8886 0.25 10.648 0.25 10.2631L0.250001 0.73686C0.250001 0.35196 0.666668 0.111397 1 0.303847L9.25 5.06699Z" FillRule="NonZero"/>
                            </Path.Data>
                        </Path>
                        <!-- Mirrored Path for previcon_Copy -->
                        <Path x:Name="previcon_Copy_mirrored" Fill="#000000" Stretch="Uniform" HorizontalAlignment="Left" Canvas.Left="5.932" VerticalAlignment="Top" Canvas.Top="3.403" Height="9" Width="6">
                            <Path.RenderTransform>
                                <ScaleTransform ScaleX="-1" CenterX="3"/>
                            </Path.RenderTransform>
                            <Path.Data>
                                <PathGeometry Figures="M9.25 5.06699C9.58333 5.25944 9.58333 5.74056 9.25 5.93301L0.999999 10.6962C0.666666 10.8886 0.25 10.648 0.25 10.2631L0.250001 0.73686C0.250001 0.35196 0.666668 0.111397 1 0.303847L9.25 5.06699Z" FillRule="NonZero"/>
                            </Path.Data>
                        </Path>
                    </Canvas>
                </Viewbox>
            </Button>




